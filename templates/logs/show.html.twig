{% extends 'base.html.twig' %}

{% block title %}Logs
{% endblock %}

{% block body %}


	<a href="{{ path('app_logs_index') }}" class="homeBtn">
		<i class="fa-solid fa-house"></i>
	</a>


	<div class="showContainer">


		<a href="{{ path('app_logs_edit', {'id': log.id}) }}" class="showEditBtn">
			<i class="fa-solid fa-pen"></i>
		</a>
		<h1>{{log.date}}</h1>
		<div class="showLogContainer">

			<img src="{{ log.img }} ">

			<p>{{ log.log }}</p>

		</div>
	</div>


	{{ include('logs/_delete_form.html.twig') }}

{% endblock %}
